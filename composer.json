{
    "name": "i3bepb/laravel-crop-str-blade-directive",
    "description": "Crop str blade directive",
    "license":"MIT",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^v6.0 || ^v7.0",
        "nunomaduro/collision": "^6.2 || ^5.0",
        "i3bepb/reflection-for-test": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "I3bepb\\CropStrBladeDirective\\": "src/",
            "I3bepb\\CropStrBladeDirective\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "I3bepb\\CropStrBladeDirective\\ServiceProvider"
            ]
        }
    }
}
